\uc0\u52572 \u51333  \u49324 \u50857 \u51088  \u46972 \u51060 \u49440 \u49828  \u44228 \u50557  \
\
\uc0\u48376  \u52572 \u51333  \u49324 \u50857 \u51088  \u46972 \u51060 \u49440 \u49828  \u44228 \u50557 ("\u44228 \u50557 ")\u51008  \u44480 \u54616 \u45716 ("\u49324 \u50857 \u51088 ") [\u51064 \u53552 \u49828 \u53440 \u51068 ] ("\u54924 \u49324 ")\u50752  \u44480 \u54616 \u51032  [\u54616 \u50864 \u49828 \u53580 \u51060 \u45320 ] \u47784 \u48148 \u51068  \u50528 \u54540 \u47532 \u52992 \u51060 \u49496 ("\u50545 ") \u49324 \u50857 \u50640  \u45824 \u54620  \u44480 \u54616 \u51032  \u51060 \u50857 \u51012  \u44508 \u51228 \u54616 \u45716  \u48277 \u51201  \u44228 \u50557 \u51077 \u45768 \u45796 . \
\
\uc0\u50545 \u51012  \u45796 \u50868 \u47196 \u46300 \u54616 \u44144 \u45208  \u49444 \u52824 \u54616 \u44144 \u45208  \u49324 \u50857 \u54632 \u51004 \u47196 \u50024 , \u44480 \u54616 \u45716  \u48376  \u44228 \u50557 \u51032  \u51312 \u44148 \u50640  \u46041 \u51032 \u54633 \u45768 \u45796 . \
\
1. \uc0\u49324 \u50857 \u51088  \u49373 \u49457  \u53080 \u53584 \u52768  \
\
\uc0\u50545 \u51008  \u49324 \u50857 \u51088 \u44032  \u53080 \u53584 \u52768 \u47484  \u49373 \u49457 \u54616 \u44256  \u51228 \u52636 \u54624  \u49688  \u51080 \u45716  \u44592 \u45733 \u51012  \u51228 \u44277 \u54633 \u45768 \u45796 ("\u49324 \u50857 \u51088  \u53080 \u53584 \u52768 "). \u49324 \u50857 \u51088  \u53080 \u53584 \u52768 \u47484  \u51228 \u52636 \u54632 \u51004 \u47196 \u50024 , \u44480 \u54616 \u45716  \u45796 \u51020 \u51012  \u46041 \u51032 \u54633 \u45768 \u45796 : \
\
\uc0\u44480 \u54616 \u44032  \u51228 \u52636 \u54620  \u53080 \u53584 \u52768 \u50640  \u45824 \u54620  \u52293 \u51076 \u51008  \u44480 \u54616 \u50640 \u44172  \u51080 \u49845 \u45768 \u45796 . \u48520 \u48277 \u51201 \u51060 \u44144 \u45208  \u54644 \u47196 \u50868 , \u50948 \u54801 \u51201 \u51060 \u44144 \u45208 , \u54617 \u45824 \u51201 \u51060 \u44144 \u45208 , \u50837 \u49444 \u51060 \u45208 , \u51020 \u46976 \u54616 \u44144 \u45208 , \u48708 \u48169 \u51201 \u51060 \u44144 \u45208 , \u44060 \u51064 \u51221 \u48372  \u52840 \u54644 \u51201 \u51060 \u44144 \u45208 , \u54800 \u50724 \u49828 \u47084 \u50868 , \u46608 \u45716  \u51064 \u51333 , \u48124 \u51313  \u46608 \u45716  \u44536  \u50808 \u50640  \u50612 \u46500  \u48169 \u49885 \u51004 \u47196 \u46304  \u48708 \u45212 \u54624  \u49688  \u51080 \u45716  \u53080 \u53584 \u52768 \u47484  \u51228 \u52636 \u54616 \u51648  \u50506 \u51012  \u44163 \u51012  \u46041 \u51032 \u54633 \u45768 \u45796 . \
\uc0\u54924 \u49324 \u50640 \u44172  \u50545  \u48143  \u54924 \u49324 \u51032  \u49324 \u50629  \u50868 \u50689 \u44284  \u44288 \u47144 \u54616 \u50668  \u49324 \u50857 \u51088  \u53080 \u53584 \u52768 \u47484  \u49324 \u50857 , \u48373 \u51228 , \u48176 \u54252 , \u54028 \u49373 \u47932  \u51089 \u49457 , \u54364 \u49884  \u48143  \u49892 \u54665 \u54624  \u49688  \u51080 \u45716  \u51204  \u49464 \u44228 \u51201 \u51060 \u44256 , \u48176 \u53440 \u51201 \u51060 \u51648  \u50506 \u51008 , \u47196 \u50676 \u54000 -\u54532 \u47532 , \u50577 \u46020  \u44032 \u45733 \u54620  \u46972 \u51060 \u49440 \u49828 \u47484  \u48512 \u50668 \u54633 \u45768 \u45796 . \
\
2. \uc0\u44552 \u51648 \u46108  \u54665 \u46041  \
\
\uc0\u44480 \u54616 \u45716  \u45796 \u51020 \u44284  \u44057 \u51008  \u44552 \u51648 \u46108  \u54665 \u46041 \u51012  \u54616 \u51648  \u50506 \u44592 \u47196  \u46041 \u51032 \u54633 \u45768 \u45796 : \
\
\uc0\u51201 \u50857  \u44032 \u45733 \u54620  \u48277 \u47456 \u51060 \u45208  \u44508 \u51221 \u51012  \u50948 \u48152 \u54616 \u45716  \u44163 . \u50612 \u46496 \u54620  \u49324 \u46988 \u51060 \u45208  \u45800 \u52404 \u47484  \u54364 \u54788 \u54616 \u44144 \u45208  \u45796 \u47480  \u49324 \u46988 \u51060 \u45208  \u45800 \u52404 \u50752 \u51032  \u51228 \u55092 \u47484  \u44144 \u51667 \u51004 \u47196  \u51452 \u51109 \u54616 \u44144 \u45208  \u44592 \u53440 \u47196 \u49436  \u45824 \u54364 \u54624  \u49688  \u50630 \u45716  \u49345 \u53468 \u47196 \u49436  \u50780 \u44257 \u54616 \u51648  \u50506 \u44592 . \u48520 \u48277 \u51201 \u51060 \u44144 \u45208  \u45796 \u47480  \u49324 \u46988 \u51032  \u44428 \u47532 \u47484  \u52840 \u54644 \u54616 \u44144 \u45208  \u44536  \u50808 \u50640  \u48277 \u47456 \u50640  \u51032 \u54644  \u44552 \u51648 \u46108  \u50612 \u46496 \u54620  \u53080 \u53584 \u52768 \u46020  \u50629 \u47196 \u46300 , \u44172 \u49884  \u46608 \u45716  \u51204 \u49569 \u54616 \u51648  \u50506 \u44592 . \u45796 \u47480  \u49324 \u50857 \u51088 \u50640  \u45824 \u54620  \u54617 \u45824 \u51201 \u51060 \u44144 \u45208  \u54644 \u47196 \u50868  \u54665 \u46041 . \u48148 \u51060 \u47084 \u49828 \u45208  \u44592 \u53440  \u50976 \u54644 \u54620  \u53076 \u46300 \u44032  \u54252 \u54632 \u46108  \u50612 \u46496 \u54620  \u53080 \u53584 \u52768 \u46020  \u50629 \u47196 \u46300 , \u44172 \u49884  \u46608 \u45716  \u51204 \u49569 \u54616 \u51648  \u50506 \u44592 . \u45796 \u47480  \u49324 \u50857 \u51088 \u47484  \u44340 \u47213 \u55176 \u44144 \u45208  \u54801 \u48149 \u54616 \u44144 \u45208  \u50948 \u54801 \u54616 \u51648  \u50506 \u44592 . \
\
3. \uc0\u54644 \u51648  \u54924 \u49324 \u45716  \u44480 \u54616 \u44032  \u48376  \u44228 \u50557 \u51012  \u50948 \u48152 \u54616 \u44144 \u45208  \u44552 \u51648 \u46108  \u54665 \u46041 \u51012  \u54616 \u44144 \u45208  \u48520 \u48277 \u51201 \u51064  \u53080 \u53584 \u52768 \u47484  \u50629 \u47196 \u46300 \u54616 \u45716  \u44221 \u50864  \u50616 \u51228 \u46304 \u51648  \u50545 \u50640  \u45824 \u54620  \u44480 \u54616 \u51032  \u50529 \u49464 \u49828 \u47484  \u51473 \u45800 \u54616 \u44144 \u45208  \u54644 \u51648 \u54624  \u44428 \u47532 \u47484  \u48372 \u50976 \u54633 \u45768 \u45796 . \
\
4. \uc0\u48372 \u51613 \u51032  \u48512 \u51064  \u50545 \u51008  \u47749 \u49884 \u51201 \u51060 \u46304  \u47925 \u49884 \u51201 \u51060 \u46304  \u50612 \u46496 \u54620  \u51333 \u47448 \u51032  \u48372 \u51613 \u46020  \u54252 \u54632 \u54616 \u51648  \u50506 \u51008  "\u51080 \u45716  \u44536 \u45824 \u47196 " \u48143  "\u49324 \u50857  \u44032 \u45733 \u54620 " \u49345 \u53468 \u47196  \u51228 \u44277 \u46121 \u45768 \u45796 . \
\
5. \uc0\u52293 \u51076  \u51228 \u54620  \u54924 \u49324 \u45716  \u44480 \u54616 \u51032  \u50545  \u49324 \u50857 \u50640  \u44288 \u47144 \u54616 \u50668  \u48156 \u49373 \u54620  \u44036 \u51217 \u51201 , \u48512 \u49688 \u51201 , \u53945 \u49688 , \u44208 \u44284 \u51201  \u46608 \u45716  \u51669 \u48268 \u51201 \u51064  \u49552 \u54644 \u50640  \u45824 \u54644  \u52293 \u51076 \u51012  \u51648 \u51648  \u50506 \u49845 \u45768 \u45796 . \
\
6. \uc0\u51201 \u50857 \u48277  \u48376  \u44228 \u50557 \u51008  \u45824 \u54620 \u48124 \u44397 \u51032  \u48277 \u47456 \u50640  \u46384 \u46972  \u54644 \u49437  \u48143  \u44508 \u51228 \u46121 \u45768 \u45796 . "\u46041 \u51032 \u54633 \u45768 \u45796 "\u47484  \u53364 \u47533 \u54616 \u44144 \u45208  \u50545 \u50640  \u50529 \u49464 \u49828 \u54616 \u44144 \u45208  \u49324 \u50857 \u54632 \u51004 \u47196 \u50024 , \u44480 \u54616 \u45716  \u48376  \u44228 \u50557 \u51012  \u51069 \u50632 \u51004 \u47728  \u51060 \u54644 \u54616 \u50688 \u51004 \u47728  \u48376  \u44228 \u50557 \u50640  \u46384 \u46972  \u44480 \u54616 \u44032  \u44480 \u49549 \u46104 \u45716  \u44163 \u50640  \u46041 \u51032 \u54616 \u45716  \u44163 \u51004 \u47196  \u44036 \u51452 \u46121 \u45768 \u45796 .}