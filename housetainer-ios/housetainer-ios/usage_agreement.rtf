\uc0\u51228 12\u51312  (\u54872 \u44553 ) \
\uc0\u54924 \u49324 \u45716  \u51060 \u50857 \u51088 \u44032  \u44396 \u47588  \u49888 \u52397 \u54620  \u49345 \u54408 \u51012 \'a0\u51228 \u44277 \u51012  \u54624  \u49688  \u50630 \u51012  \u46412 \u50640 \u45716  \u51648 \u52404  \u50630 \u51060  \u44536  \u49324 \u50976 \u47484  \u51060 \u50857 \u51088 \u50640 \u44172  \u53685 \u51648 \u54616 \u44256  \u49324 \u51204 \u50640  \u49345 \u54408 \u51032  \u45824 \u44552 \u51012  \u48155 \u51008  \u44221 \u50864 \u50640 \u45716  \u45824 \u44552 \u51012  \u48155 \u51008  \u45216 \u48512 \u53552  3\u50689 \u50629 \u51068  \u51060 \u45236 \u50640  \u54872 \u44553 \u54616 \u44144 \u45208  \u54872 \u44553 \u50640  \u54596 \u50836 \u54620  \u51312 \u52824 \u47484  \u52712 \u54633 \u45768 \u45796 . \u51228 13\u51312  (\u52397 \u50557 \u52384 \u54924 ) \u9312 \'a0\u54924 \u49324 \u50752  \u49345 \u54408 \u51032  \u44396 \u47588 \u50640  \u44288 \u54620  \u44228 \u50557 \u51012  \u52404 \u44208 \u54620  \u51060 \u50857 \u51088 \u45716  \u44228 \u50557 \u45236 \u50857 \u50640  \u44288 \u54620  \u49436 \u47732 \u51012  \u48155 \u51008  \u45216 \u47196 \u48512 \u53552  30\u51068  \u51060 \u45236 \u50640  \u52397 \u50557 \u52384 \u54924 \u47484  \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u52397 \u50557 \u52384 \u54924 \u50640  \u44288 \u54616 \u50668  \u12300 \u51204 \u51088 \u49345 \u44144 \u47000  \u46321 \u50640 \u49436 \u51032  \u49548 \u48708 \u51088 \u48372 \u54840 \u50640  \u44288 \u54620  \u48277 \u47456 \u12301 \u50640  \u45804 \u47532  \u51221 \u54632 \u51060  \u51080 \u45716  \u44221 \u50864 \u50640 \u45716  \u46041  \u48277  \u44508 \u51221 \u50640  \u46384 \u47493 \u45768 \u45796 .\u8232 \u8232  \u51228 14\u51312  (\u52397 \u50557 \u52384 \u54924  \u46321 \u51032  \u54952 \u44284 ) \'a0\
\uc0\u54924 \u49324 \u45716  \u51060 \u50857 \u51088 \u47196 \u48512 \u53552  \u49345 \u54408 \u51012  \u48152 \u54872  \u48155 \u51008  \u44221 \u50864  3\u50689 \u50629 \u51068  \u51060 \u45236 \u50640  \u51060 \u48120  \u51648 \u44553 \u48155 \u51008  \u49345 \u54408 \u51032  \u45824 \u44552 \u51012  \u54872 \u44553 \u54633 \u45768 \u45796 . \u51060  \u44221 \u50864  \u54924 \u49324 \u44032  \u51060 \u50857 \u51088 \u50640 \u44172  \u49345 \u54408  \u45824 \u44552 \u51032  \u54872 \u44553 \u51012  \u51648 \u50672 \u54620  \u46412 \u50640 \u45716  \u44536  \u51648 \u50672 \u44592 \u44036 \u50640  \u45824 \u54616 \u50668  \u12300 \u51204 \u51088 \u49345 \u44144 \u47000  \u46321 \u50640 \u49436 \u51032  \u49548 \u48708 \u51088 \u48372 \u54840 \u50640  \u44288 \u54620  \u48277 \u47456  \u49884 \u54665 \u47161 \u12301  \u51228 21\u51312 \u51032 3\u50640 \u49436  \u51221 \u54616 \u45716  \u51648 \u50672 \u51060 \u51088 \u50984 \u51012  \u44273 \u54616 \u50668  \u49328 \u51221 \u54620  \u51648 \u50672 \u51060 \u51088 \u47484  \u51648 \u44553 \u54633 \u45768 \u45796 . \u54924 \u49324 \u45716  \u50948  \u45824 \u44552 \u51012  \u54872 \u44553 \u54632 \u50640  \u51080 \u50612 \u49436  \u51060 \u50857 \u51088 \u44032  \u49888 \u50857 \u52852 \u46300  \u46608 \u45716  \u51204 \u51088 \u54868 \u54224  \u46321 \u51032  \u44208 \u51228 \u49688 \u45800 \u51004 \u47196  \u49345 \u54408 \u51032  \u45824 \u44552 \u51012  \u51648 \u44553 \u54620  \u46412 \u50640 \u45716  \u51648 \u52404  \u50630 \u51060  \u45817 \u54644  \u44208 \u51228 \u49688 \u45800 \u51012  \u51228 \u44277 \u54620  \u49324 \u50629 \u51088 \u47196  \u54616 \u50668 \u44552  \u49345 \u54408 \u51032  \u45824 \u44552 \u51032  \u52397 \u44396 \u47484  \u51221 \u51648  \u46608 \u45716  \u52712 \u49548 \u54616 \u46020 \u47197  \u50836 \u52397 \u54633 \u45768 \u45796 . \u52397 \u50557 \u52384 \u54924  \u46321 \u51032  \u44221 \u50864  \u44277 \u44553 \u48155 \u51008  \u49345 \u54408 \u51032  \u48152 \u54872 \u50640  \u54596 \u50836 \u54620  \u48708 \u50857 \u51008  \u51060 \u50857 \u51088 \u44032  \u48512 \u45812 \u54633 \u45768 \u45796 . \u54924 \u49324 \u45716  \u51060 \u50857 \u51088 \u50640 \u44172  \u52397 \u50557 \u52384 \u54924  \u46321 \u51012  \u51060 \u50976 \u47196  \u50948 \u50557 \u44552  \u46608 \u45716  \u49552 \u54644 \u48176 \u49345 \u51012  \u52397 \u44396 \u54616 \u51648  \u50506 \u49845 \u45768 \u45796 . \
