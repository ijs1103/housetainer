\uc0\u51228 3\u51312  (\u50557 \u44288 \u51032  \u44172 \u49884 \u50752  \u48320 \u44221 ) \
\uc0\u54924 \u49324 \u45716  \u48376  \u50557 \u44288 \u51032  \u45236 \u50857 \u44284  \u49345 \u54840 , \u45824 \u54364 \u51088  \u49457 \u47749 , \u50689 \u50629 \u49548  \u49548 \u51116 \u51648  \u51452 \u49548 (\u49548 \u48708 \u51088 \u51032  \u48520 \u47564 \u51012  \u52376 \u47532 \u54624  \u49688  \u51080 \u45716  \u44275 \u51032  \u51452 \u49548 \u47484  \u54252 \u54632 ), \u51204 \u54868 \u48264 \u54840 , \u51060 \u47700 \u51068  \u51452 \u49548 , \u54840 \u49828 \u54021  \u49436 \u48708 \u49828  \u49324 \u50629 \u51088 , \u49324 \u50629 \u51088 \u46321 \u47197 \u48264 \u54840 , \u53685 \u49888 \u54032 \u47588 \u50629 \u49888 \u44256 \u48264 \u54840 , \u44060 \u51064 \u51221 \u48372 \u52376 \u47532 \u48169 \u52840  \u46321 \u51012  \u51060 \u50857 \u51088 \u44032  \u49789 \u44172  \u50508  \u49688  \u51080 \u46020 \u47197  \u50545 \u51032  \u52488 \u44592 \u54868 \u47732 \u50640  \u44172 \u49884 \u54633 \u45768 \u45796 . \u45796 \u47564 , \u54924 \u49324 \u45716  \u51060 \u50857 \u51088 \u44032  \u48376  \u50557 \u44288  \u48143  \u44060 \u51064 \u51221 \u48372 \u52376 \u47532 \u48169 \u52840 \u51032  \u45236 \u50857 \u51012  \u52488 \u44592 \u54868 \u47732 \u51032  \u50672 \u44208 \u54868 \u47732 \u51012  \u53685 \u54616 \u50668  \u48380  \u49688  \u51080 \u46020 \u47197  \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u54924 \u49324 \u45716  \u12300 \u51204 \u51088 \u49345 \u44144 \u47000  \u46321 \u50640 \u49436 \u51032  \u49548 \u48708 \u51088 \u48372 \u54840 \u50640  \u44288 \u54620  \u48277 \u47456 \u12301 , \u12300 \u50557 \u44288 \u51032  \u44508 \u51228 \u50640  \u44288 \u54620  \u48277 \u47456 \u12301 , \u12300 \u51204 \u51088 \u47928 \u49436  \u48143  \u51204 \u51088 \u44144 \u47000 \u44592 \u48376 \u48277 \u12301 , \u12300 \u51204 \u51088 \u44552 \u50997 \u44144 \u47000 \u48277 \u12301 , \u12300 \u51204 \u51088 \u49436 \u47749 \u48277 \u12301 , \u12300 \u51221 \u48372 \u53685 \u49888 \u47581  \u51060 \u50857 \u52489 \u51652  \u48143  \u51221 \u48372 \u48372 \u54840  \u46321 \u50640  \u44288 \u54620  \u48277 \u47456 \u12301 , \u12300 \u48169 \u47928 \u54032 \u47588  \u46321 \u50640  \u44288 \u54620  \u48277 \u47456 \u12301 , \u12300 \u49548 \u48708 \u51088 \u44592 \u48376 \u48277 \u12301  \u46321  \u44288 \u47144  \u48277 \u51012  \u50948 \u48176 \u54616 \u51648  \u50506 \u45716  \u48276 \u50948 \u50640 \u49436  \u48376  \u50557 \u44288 \u51012  \u44060 \u51221 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u54924 \u49324 \u45716  \u48376  \u50557 \u44288 \u51012  \u44060 \u51221 \u54624  \u44221 \u50864 \u50640 \u45716  \u51201 \u50857 \u51068 \u51088  \u48143  \u44060 \u51221 \u49324 \u50976 \u47484  \u47749 \u49884 \u54616 \u50668  \u54788 \u54665 \u50557 \u44288 \u44284  \u54632 \u44760  \u50545 \u51032  \u54868 \u47732 \u50640  \u44536  \u51201 \u50857 \u51068 \u51088  7\u51068  \u51060 \u51204 \u48512 \u53552  \u51201 \u50857 \u51068 \u51088  \u51204 \u51068 \u44620 \u51648  \u44277 \u51648 \u54633 \u45768 \u45796 . \u45796 \u47564 , \u51060 \u50857 \u51088 \u50640 \u44172  \u48520 \u47532 \u54616 \u44172  \u50557 \u44288 \u45236 \u50857 \u51012  \u48320 \u44221 \u54616 \u45716  \u44221 \u50864 \u50640 \u45716  \u52572 \u49548 \u54620  30\u51068  \u51060 \u49345 \u51032  \u49324 \u51204  \u50976 \u50696 \u44592 \u44036 \u51012  \u46160 \u44256  \u44277 \u51648 \u54633 \u45768 \u45796 . \u51060  \u44221 \u50864  \u54924 \u49324 \u45716  \u44060 \u51221  \u51204  \u45236 \u50857 \u44284  \u44060 \u51221  \u54980  \u45236 \u50857 \u51012  \u47749 \u54869 \u54616 \u44172  \u48708 \u44368 \u54616 \u50668  \u51060 \u50857 \u51088 \u44032  \u50508 \u44592  \u49789 \u46020 \u47197  \u54364 \u49884 \u54633 \u45768 \u45796 . \u48320 \u44221 \u46108  \u50557 \u44288  \u51312 \u54637  \u51473  \u49345 \u54408 \u51032  \u44396 \u47588 \u44228 \u50557 \u50640  \u44288 \u54620  \u51312 \u54637 \u51008  \u44536  \u51201 \u50857 \u51068 \u51088  \u51060 \u54980 \u50640  \u52404 \u44208 \u46104 \u45716  \u44228 \u50557 \u50640 \u47564  \u51201 \u50857 \u46104 \u44256  \u44536  \u51060 \u51204 \u50640  \u51060 \u48120  \u52404 \u44208 \u46108  \u44228 \u50557 \u50640  \u45824 \u54644 \u49436 \u45716  \u48320 \u44221  \u51204 \u51032  \u51312 \u54637 \u51060  \u44536 \u45824 \u47196  \u51201 \u50857 \u46121 \u45768 \u45796 . \u45796 \u47564 , \u51060 \u48120  \u49345 \u54408 \u51032  \u44396 \u47588 \u44228 \u50557 \u51012  \u52404 \u44208 \u54620  \u51060 \u50857 \u51088 \u44032  \u48320 \u44221 \u46108  \u51312 \u54637 \u51032  \u51201 \u50857 \u51012  \u48155 \u44592 \u47484  \u50896 \u54616 \u45716  \u46907 \u51012  \u49345 \u44592  \u51228 4\u54637 \u50640  \u51032 \u54620  \u48320 \u44221 \u50557 \u44288 \u51032  \u44277 \u51648 \u44592 \u44036  \u45236 \u50640  \u54924 \u49324 \u50640  \u44256 \u51648 \u54616 \u50668  \u54924 \u49324 \u51032  \u46041 \u51032 \u47484  \u48155 \u51008  \u44221 \u50864 \u50640 \u45716  \u48320 \u44221 \u46108  \u51312 \u54637 \u51060  \u51201 \u50857 \u46121 \u45768 \u45796 . \u54924 \u49324 \u44032  \u44060 \u51221 \u50557 \u44288 \u51012  \u44277 \u51648  \u46608 \u45716  \u53685 \u51648 \u54616 \u47732 \u49436  \u54924 \u50896 \u50640 \u44172  \u51068 \u51221 \u44592 \u44036  \u45236 \u50640  \u44060 \u51221 \u50557 \u44288 \u50640  \u45824 \u54620  \u53945 \u48324 \u54620  \u51032 \u49324 \u54364 \u49884 \u47484  \u54616 \u51648  \u50506 \u51004 \u47732  \u46041 \u51032 \u54620  \u44163 \u51004 \u47196  \u48376 \u45796 \u45716  \u46907 \u51012  \u47749 \u54869 \u54616 \u44172  \u46384 \u47196  \u44277 \u51648  \u46608 \u45716  \u53685 \u51648 \u54616 \u50688 \u51020 \u50640 \u46020  \u54924 \u50896 \u51060  \u47749 \u49884 \u51201 \u51004 \u47196  \u44144 \u48512 \u51032 \u49324 \u47484  \u54364 \u49884 \u54616 \u51648  \u50500 \u45768 \u54620  \u44221 \u50864  \u54924 \u50896 \u51060  \u44060 \u51221 \u50557 \u44288 \u50640  \u46041 \u51032 \u54620  \u44163 \u51004 \u47196  \u48389 \u45768 \u45796 . \u54924 \u50896 \u51060  \u44060 \u51221 \u50557 \u44288 \u51032  \u51201 \u50857 \u50640  \u46041 \u51032 \u54616 \u51648  \u50506 \u45716  \u44221 \u50864  \u54924 \u49324 \u45716  \u44060 \u51221 \u50557 \u44288 \u51032  \u45236 \u50857 \u51012  \u51201 \u50857 \u54624  \u49688  \u50630 \u51004 \u47728 , \u54924 \u50896 \u51008  \u48376  \u50557 \u44288 \u51012  \u53685 \u54620  \u51060 \u50857 \u44228 \u50557 \u51012  \u54644 \u51648 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45796 \u47564 , \u44592 \u51316 \u50557 \u44288 \u51012  \u51201 \u50857 \u54624  \u49688  \u50630 \u45716  \u53945 \u48324 \u54620  \u49324 \u51221 \u51060  \u51080 \u45716  \u44221 \u50864 \u50640 \u45716  \u54924 \u49324 \u45716  \u48376  \u50557 \u44288 \u51012  \u53685 \u54620  \u51060 \u50857 \u44228 \u50557 \u51012  \u54644 \u51648 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u48376  \u50557 \u44288 \u50640 \u49436  \u51221 \u54616 \u51648  \u50500 \u45768 \u54620  \u45236 \u50857 \u44284  \u48376  \u50557 \u44288 \u51032  \u54644 \u49437 \u50640  \u44288 \u54616 \u50668 \u45716  \u12300 \u51204 \u51088 \u49345 \u44144 \u47000  \u46321 \u50640 \u49436 \u51032  \u49548 \u48708 \u51088 \u48372 \u54840 \u50640  \u44288 \u54620  \u48277 \u47456 \u12301 , \u12300 \u50557 \u44288 \u51032  \u44508 \u51228 \u50640  \u44288 \u54620  \u48277 \u47456 \u12301 , \u44277 \u51221 \u44144 \u47000 \u50948 \u50896 \u54924 \u44032  \u51221 \u54616 \u45716  \u12300 \u51204 \u51088 \u49345 \u44144 \u47000  \u46321 \u50640 \u49436 \u51032  \u49548 \u48708 \u51088 \u48372 \u54840  \u51648 \u52840 \u12301  \u48143  \u44288 \u44228 \u48277 \u47161  \u46608 \u45716  \u49345 \u44288 \u47168 \u50640  \u46384 \u47493 \u45768 \u45796 . \'a0 \
\
\uc0\u51228 4\u51312  (\u50545 \u51032  \u51473 \u45800 ) \
\uc0\u9312 \'a0\u54924 \u49324 \u45716  \u52980 \u54504 \u53552  \u46321  \u51204 \u44592 \u53685 \u49888 \u49444 \u48708 \u51032  \u48372 \u49688 \u51216 \u44160 \'b7\u44368 \u52404  \u48143  \u44256 \u51109 , \u53685 \u49888 \u51032  \u46160 \u51208  \u46321 \u51032  \u49324 \u50976 \u44032  \u48156 \u49373 \u54620  \u44221 \u50864 , \u46608 \u45716  \u50545 \u51032  \u50868 \u50689 \u44284  \u44288 \u47144 \u54616 \u50668  \u54924 \u49324 \u44032  \u54032 \u45800 \u54620  \u44288 \u47532 \u51201 , \u44592 \u49696 \u51201  \u54596 \u50836 \u44032  \u51080 \u45716  \u44221 \u50864 , \u50545 \u51032  \u51228 \u44277 \u51012  \u51068 \u49884 \u51201  \u54841 \u51008  \u50689 \u44396 \u51201 \u51004 \u47196  \u51473 \u45800 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u8232 \u9313 \'a0\u51228 1\u54637 \u50640  \u51032 \u54620  \u50545  \u51473 \u45800 \u51060  \u48156 \u49373 \u54616 \u45716  \u44221 \u50864 , \u54924 \u49324 \u45716  \u51228 9\u51312 \u50640  \u51221 \u54620  \u48169 \u48277 \u51004 \u47196  \u54644 \u45817  \u49324 \u49892 \u51012  \u51060 \u50857 \u51088 \u50640 \u44172  \u53685 \u51648 \u54633 \u45768 \u45796 . \
\
\uc0\u51228 5\u51312  (\u54924 \u50896 \u44032 \u51077 ) \'a0\
\uc0\u51060 \u50857 \u51088 \u45716  \u47924 \u47308 \u47196  \u54924 \u50896 \u51004 \u47196  \u44032 \u51077 \u54624  \u49688  \u51080 \u51004 \u47728 , \u54924 \u49324 \u44032  \u51221 \u54620  \u44032 \u51077  \u50577 \u49885 \u50640  \u54924 \u50896 \u51221 \u48372 \u47484  \u44592 \u51077 \u54620  \u54980  \u48376  \u50557 \u44288 \u44284  \u44060 \u51064 \u51221 \u48372 \u51032  \u49688 \u51665 , \u51228 \u44277  \u48143  \u54876 \u50857 \u50640  \u46041 \u51032 \u54620 \u45796 \u45716  \u51032 \u49324 \u54364 \u49884 \u47484  \u54632 \u51004 \u47196 \u49436  \u54924 \u50896 \u44032 \u51077 \u51012  \u49888 \u52397 \u54633 \u45768 \u45796 . \u54924 \u49324 \u45716  \u51228 1\u54637 \u50640  \u46384 \u46972  \u54924 \u50896 \u44032 \u51077 \u51012  \u49888 \u52397 \u54620  \u51060 \u50857 \u51088 \u50640 \u44172  \u45796 \u51020  \u44033  \u54840 \u51032  \u49324 \u50976 \u44032  \u51064 \u51221 \u46104 \u45716  \u44221 \u50864  \u44536  \u49324 \u50976 \u44032  \u54644 \u49548 \u46112  \u46412 \u44620 \u51648  \u54924 \u50896 \u46321 \u47197 \u51012  \u44144 \u48512 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u8232 1.\'a0\'a0 \'a0\u44032 \u51077 \u51012  \u49888 \u52397 \u54620  \u51060 \u50857 \u51088 \u44032  \u48376  \u50557 \u44288  \u51228 6\u51312  \u51228 3\u54637 \u50640  \u51032 \u54616 \u50668  \u51060 \u51204 \u50640  \u54924 \u50896 \u51088 \u44201 \u51012  \u49345 \u49892 \u54620  \u51201 \u51060  \u51080 \u45716  \u44221 \u50864 \u8232 2.\'a0\'a0 \'a0\u46321 \u47197  \u45236 \u50857 \u50640  \u54728 \u50948 , \u44592 \u51116 \u45572 \u46973 , \u50724 \u44592 \u44032  \u51080 \u45716  \u44221 \u50864 \u8232 3.\'a0\'a0 \'a0\u48376  \u50557 \u44288  \u51228 6\u51312  \u51228 3\u54637  \u44033  \u54840 \u50640  \u54644 \u45817 \u54616 \u45716  \u49324 \u50976 \u44032  \u51080 \u44144 \u45208 , \u44536 \u50752  \u44057 \u51008  \u49324 \u50976 \u44032  \u51080 \u51012  \u44163 \u51004 \u47196  \u48380  \u49345 \u45817 \u54620  \u50864 \u47140