\uc0\u51228 10\u51312  (\u51648 \u44553 \u48169 \u48277 ) \
\uc0\u9312 \'a0\u54924 \u49324 \u45716  \u51060 \u50857 \u51088 \u44032  \u44396 \u47588 \u54620  \u49345 \u54408 \u50640  \u45824 \u54620  \u45824 \u44552 \u51648 \u44553 \u48169 \u48277 \u51012  \u45796 \u51020  \u44033  \u54840 \u51032  \u48169 \u48277  \u51473  \u50612 \u45712  \u54616 \u45208 \u47196  \u51228 \u44277 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u45800 , \u45824 \u44552 \u51648 \u44553 \u48169 \u48277 \u51008  \u54924 \u49324 \u51032  \u45236 \u48512  \u51221 \u52293  \u48143  \u44288 \u47532 \u51201 , \u44592 \u49696 \u51201  \u49324 \u50976 \u50640  \u46384 \u46972 \u49436  \u48320 \u44221 \u46112  \u49688  \u51080 \u49845 \u45768 \u45796 .\u8232 1.\'a0\'a0 \'a0\u54256 \u48197 \u53433 , \u51064 \u53552 \u45367 \u48197 \u53433  \u46321 \u51032  \u44033 \u51333  \u44228 \u51340 \u51060 \u52404 \u8232 2.\'a0\'a0 \'a0\u49440 \u48520 \u52852 \u46300 , \u51649 \u48520 \u52852 \u46300 , \u49888 \u50857 \u52852 \u46300  \u46321 \u51032  \u44033 \u51333  \u52852 \u46300  \u44208 \u51228 \u8232 3.\'a0\'a0 \'a0\u51204 \u51088 \u54868 \u54224 \u50640  \u51032 \u54620  \u44208 \u51228 \u8232 4.\'a0\'a0 \'a0\u54924 \u49324 \u50752  \u44228 \u50557 \u51012  \u47610 \u50632 \u44144 \u45208  \u54924 \u49324 \u44032  \u51064 \u51221 \u54620  \u49345 \u54408 \u44428 \u50640  \u51032 \u54620  \u44208 \u51228 \u8232 5.\'a0\'a0 \'a0\u44592 \u53440  \u51204 \u51088 \u51201  \u51648 \u44553  \u48169 \u48277 \u50640  \u51032 \u54620  \u45824 \u44552  \u51648 \u44553  \u46321 \u8232 \u9313 \'a0\u54924 \u49324 \u45716  \u51060 \u50857 \u51088 \u51032  \u44396 \u47588 \u44552 \u50529 \u50640  \u45824 \u54616 \u50668  \u45236 \u48512 \u51221 \u52293  \u48143  \u50808 \u48512  \u44208 \u51228 \u50629 \u52404 (\u51008 \u54665 \u49324 , \u52852 \u46300 \u49324  \u46321 ), \u44592 \u53440  \u44288 \u47144  \u48277 \u47161 \u51032  \u48320 \u44221 \u50640  \u46384 \u46972  \u50900  \u45572 \u51201  \u44208 \u51228 \u50529 , \u44208 \u51228 \u54620 \u46020  \u46321 \u51032  \u44144 \u47000 \u54620 \u46020 \u47484  \u51221 \u54624  \u49688  \u51080 \u51004 \u47728 , \u54644 \u45817  \u51060 \u50857 \u51088 \u45716  \u44536  \u54620 \u46020 \u47484  \u52488 \u44284 \u54616 \u45716  \u48276 \u50948 \u50640 \u49436  \u49345 \u54408 \u51012  \u44396 \u47588 \u54616 \u44256 \u51088  \u54624  \u44221 \u50864  \u44144 \u47000 \u54620 \u46020 \u51032  \u52488 \u44284 \u47196  \u51064 \u54616 \u50668  \u49345 \u54408 \u44396 \u47588 \u44032  \u48520 \u44032 \u45733 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \
