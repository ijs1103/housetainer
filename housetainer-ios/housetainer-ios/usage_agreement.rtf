\uc0\u51228 17\u51312  (\u44552 \u51648 \u54665 \u50948 ) \
\uc0\u51060 \u50857 \u51088 \u45716  \u45796 \u51020  \u54665 \u50948 \u47484  \u54616 \u50668 \u49436 \u45716  \u50504  \u46121 \u45768 \u45796 .\u8232 1.\'a0\'a0 \'a0\u54924 \u50896 \u51221 \u48372 \u51032  \u46321 \u47197  \u46608 \u45716  \u49688 \u51221  \u49884  \u54728 \u50948 \u45236 \u50857  \u46608 \u45716  \u47749 \u48177 \u54616 \u44172  \u51096 \u47803 \u46108  \u45236 \u50857 \u51012  \u46321 \u47197 \u54616 \u45716  \u54665 \u50948 \u8232 2.\'a0\'a0 \'a0\u53440 \u51064 \u51032  ID\u50752  \u48708 \u48128 \u48264 \u54840  \u46608 \u45716  \u53440 \u51064 \u51032  \u44060 \u51064 \u51221 \u48372 \u47484  \u46020 \u50857 \u54616 \u45716  \u54665 \u50948 \u8232 3.\'a0\'a0 \'a0\u54924 \u50896 \u51032  ID\u50752  \u48708 \u48128 \u48264 \u54840 \u47484  \u51228 3\u51088 \u50640 \u44172  \u50577 \u46020 , \u45824 \u50668 \u54616 \u44144 \u45208  \u51228 3\u51088 \u44032  \u51060 \u50857 \u54616 \u46020 \u47197  \u54728 \u46973 \u54616 \u45716  \u54665 \u50948 \u8232 4.\'a0\'a0 \'a0\u50545 \u50640  \u44172 \u49884 \u46108  \u51221 \u48372 \u47484  \u44428 \u54620 \u50630 \u51060  \u51076 \u51032 \u47196  \u48373 \u51228 , \u49688 \u51221 , \u48320 \u44221 , \u44060 \u51089 , \u44277 \u44060 \u54616 \u45716  \u54665 \u50948 \u8232 5.\'a0\'a0 \'a0\u54924 \u49324 \u44032  \u54728 \u46973 \u54616 \u51648  \u50506 \u51008  \u51221 \u48372 (\u52980 \u54504 \u53552  \u54532 \u47196 \u44536 \u47016  \u46321 )\u51032  \u49569 \u49888  \u46608 \u45716  \u44172 \u49884 \u54616 \u45716  \u54665 \u50948 \u8232 6.\'a0\'a0 \'a0\u54924 \u49324  \u44592 \u53440  \u51228 3\u51088 \u51032  \u51200 \u51089 \u44428  \u46321  \u51648 \u51201 \u51116 \u49328 \u44428 \u50640  \u45824 \u54620  \u52840 \u54644 \u54616 \u45716  \u54665 \u50948 \u8232 7.\'a0\'a0 \'a0\u54924 \u49324  \u44592 \u53440  \u51228 3\u51088 \u51032  \u47749 \u50696 \u47484  \u48512 \u45817 \u54616 \u44172  \u49552 \u49345 \u49884 \u53412 \u44144 \u45208  \u48520 \u51060 \u51061 \u51012  \u51452 \u45716  \u54665 \u50948 \u8232 8.\'a0\'a0 \'a0\u54924 \u49324 \u51032  \u50629 \u47924  \u46608 \u45716  \u51228 3\u51088 \u51032  \u50545  \u51060 \u50857 \u51012  \u48512 \u45817 \u54616 \u44172  \u48169 \u54644 \u54616 \u45716  \u54665 \u50948 \u8232 9.\'a0\'a0 \'a0\u54924 \u49324 \u50640  \u45824 \u54616 \u50668  \u54633 \u47532 \u51201 \u51064  \u49324 \u50976  \u50630 \u51060  \u49345 \u49845 \u51201 \u51004 \u47196  \u53364 \u47112 \u51076 \u51012  \u51228 \u44592 \u54616 \u50668  \u54924 \u49324 \u51032  \u50629 \u47924 \u50640  \u51648 \u51109 \u51012  \u51452 \u44144 \u45208  \u50837 \u49444 , \u49457 \u51201  \u49688 \u52824 \u49900 \u51012  \u50976 \u48156 \u54616 \u45716  \u54364 \u54788  \u46321 \u51012  \u49324 \u50857 \u54616 \u50668  \u53364 \u47112 \u51076 \u51012  \u51228 \u44592 \u54616 \u45716  \u54665 \u50948 \u8232 10.\'a0\'a0 \'a0\u54924 \u49324 \u44032  \u51228 \u44277 \u54616 \u45716  \u50545  \u51060 \u50857 \u48169 \u48277 \u50640  \u51032 \u54616 \u51648  \u50500 \u45768 \u54616 \u44256  \u48708 \u51221 \u49345 \u51201 \u51064  \u48169 \u48277 \u51004 \u47196  \u50545 \u51012  \u51060 \u50857 \u54616 \u44144 \u45208  \u44288 \u47144  \u49884 \u49828 \u53596 \u50640  \u51217 \u44540 \u54616 \u45716  \u54665 \u50948 \u8232 11.\'a0\'a0 \'a0\u50545 \u50640  \u50808 \u49444  \u46608 \u45716  \u54253 \u47141 \u51201 \u51064  \u47700 \u49884 \u51648 \'b7\u54868 \u49345 \'b7\u51020 \u49457  \u44592 \u53440  \u44277 \u49436 \u50577 \u49549 \u50640  \u48152 \u54616 \u45716  \u51221 \u48372 \u47484  \u54868 \u47732 \u50640  \u44277 \u44060  \u46608 \u45716  \u44172 \u49884 \u54616 \u45716  \u54665 \u50948 \u8232 12.\'a0\'a0 \'a0\u54788 \u44552 \u51012  \u50997 \u53685 \u54616 \u44592  \u50948 \u54620  \u47785 \u51201 \u51004 \u47196  \u50668 \u49888 \u51204 \u47928 \u44552 \u50997 \u50629 \u48277  \u46321  \u48277 \u47161 \u50640  \u46384 \u46972  \u44552 \u51648 \u46108  \u48169 \u48277 \u51004 \u47196  \u45824 \u44552 \u51012  \u44208 \u51228 \u54616 \u44256  \