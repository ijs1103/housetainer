\uc0\u51060 \u50857 \u51088 \u45716  \u50545 \u50640 \u49436  \u45796 \u51020  \u46608 \u45716  \u51060 \u50752  \u50976 \u49324 \u54620  \u48169 \u48277 \u50640  \u51032 \u54616 \u50668  \u44396 \u47588 \u47484  \u49888 \u52397 \u54616 \u47728 , \u54924 \u49324 \u45716  \u51060 \u50857 \u51088 \u44032  \u44396 \u47588 \u49888 \u52397 \u51012  \u54632 \u50640  \u51080 \u50612 \u49436  \u49345 \u54408  \u46321 \u50640  \u45824 \u54620  \u44144 \u47000 \u51312 \u44148 \u51012  \u51221 \u54869 \u54616 \u44172  \u51060 \u54644 \u54616 \u44256  \u44144 \u47000 \u54624  \u49688  \u51080 \u46020 \u47197  \u45796 \u51020 \u51032  \u44033  \u44288 \u47144  \u45236 \u50857 \u51012  \u54252 \u54632 \u54616 \u50668  \u44288 \u44228 \u48277 \u47161 \u49345  \u50836 \u44396 \u54616 \u45716  \u51221 \u48372 \u47484  \u50508 \u44592  \u49789 \u44172  \u54364 \u49884  \u48143  \u44256 \u51648 \u54633 \u45768 \u45796 .\u8232 1.\'a0\u50557 \u44288 \u45236 \u50857 , \u52397 \u50557 \u52384 \u54924  \u51228 \u54620 \u49345 \u54408 (\u54644 \u45817 \u54616 \u45716  \u51228 \u54408 \u51032  \u44221 \u50864 ), \u46321 \u51032  \u48708 \u50857 \u48512 \u45812  \u46321  \u44396 \u47588 \u51312 \u44148 \u44284  \u44288 \u47144 \u54620  \u45236 \u50857 \u50640  \u45824 \u54620  \u54869 \u51064 \u8232 2. \u44396 \u47588 \u51312 \u44148 \u50640  \u45824 \u54616 \u50668  \u46041 \u51032 \u54616 \u44144 \u45208  \u44144 \u48512 \u54616 \u45716  \u54364 \u49884 (\u50696 , \u47560 \u50864 \u49828  \u53364 \u47533 )\u8232 3.\'a0\u49345 \u54408 \u51032  \u44396 \u47588 \u49888 \u52397  \u48143  \u51060 \u50640  \u44288 \u54620  \u54869 \u51064  \u46608 \u45716  \u54924 \u49324 \u51032  \u54869 \u51064 \u50640  \u45824 \u54620  \u46041 \u51032 \u8232 4.\'a0\u44208 \u51228 \u48169 \u48277 \u51032  \u49440 \u53469  \u48143  \u44208 \u51228 \u8232 \u9313 \'a0\u54924 \u49324 \u45716  \u51060 \u50857 \u51088 \u44032  \u51228 1\u54637 \u50640  \u46384 \u46972  \u44396 \u47588 \u49888 \u52397  \u49884  \u51060 \u50857 \u51088 \u47196 \u48512 \u53552  \u44060 \u51064 \u51221 \u48372 \u51032  \u49688 \u51665 , \u51060 \u50857 , \u51228 \u44277  \u48143  \u50948 \u53441 \u50640  \u44288 \u54616 \u50668  \u46041 \u51032 \u47484  \u48155 \u51012  \u49688  \u51080 \u49845 \u45768 \u45796 . \u51060 \u46412  \u54924 \u49324 \u45716  \u44288 \u44228 \u48277 \u47161 \u49345  \u50836 \u44396 \u46104 \u45716  \u49324 \u54637 \u46308 \u51012  \u54644 \u45817  \u51060 \u50857 \u51088 \u50640 \u44172  \u44256 \u51648 \u54616 \u44256  \u46041 \u51032 \u47484  \u48155 \u49845 \u45768 \u45796 . \u54924 \u49324 \u45716  \u51060 \u50857 \u51088 \u51032  \u44060 \u51064 \u51221 \u48372  \u52376 \u47532 \u50752  \u44288 \u47144 \u54616 \u50668  \u44288 \u44228 \u48277 \u47161 \u51012  \u51456 \u49688 \u54616 \u44256  \u48376  \u50557 \u44288  \u51228 15\u51312  \u48143  \u44060 \u51064 \u51221 \u48372  \u52376 \u47532 \u48169 \u52840 \u50640  \u46384 \u47493 \u45768 \u45796 .\u8232 \
\uc0\u51228 9\u51312  (\u44396 \u47588 \u44228 \u50557 \u51032  \u49457 \u47549 ) \'a0\
\uc0\u54924 \u49324 \u45716  \u44033  \u54840 \u50640  \u54644 \u45817 \u54616 \u45716  \u49324 \u50976 \u44032  \u51316 \u51116 \u54616 \u44144 \u45208  \u51316 \u51116 \u54616 \u45716  \u44163 \u51004 \u47196  \u48380 \u47564 \u54620  \u49345 \u45817 \u54620  \u49324 \u51221 \u51060  \u51316 \u51116 \u54616 \u45716  \u44221 \u50864 , \u51228 8\u51312 \u50640  \u46384 \u47480  \u51060 \u50857 \u51088 \u51032  \u44396 \u47588 \u49888 \u52397 \u51012  \u49849 \u45209 \u54616 \u51648  \u50506 \u44144 \u45208  \u52712 \u49548 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 . \u46608 \u54620 , \u54924 \u49324 \u44032  19\u49464  \u48120 \u47564 \u51032  \u48120 \u49457 \u45380 \u51088 \u50752  \u44228 \u50557 \u51012  \u52404 \u44208 \u54616 \u45716  \u44221 \u50864 \u50640 \u45716  \u48277 \u51221 \u45824 \u47532 \u51064 \u51032  \u46041 \u51032 \u47484  \u50619 \u51648  \u47803 \u54616 \u47732  \u48120 \u49457 \u45380 \u51088  \u48376 \u51064  \u46608 \u45716  \u48277 \u51221 \u45824 \u47532 \u51064 \u51060  \u44228 \u50557 \u51012  \u52712 \u49548 \u54624  \u49688  \u51080 \u49845 \u45768 \u45796 .\u8232 1. \u49888 \u52397  \u45236 \u50857 \u50640  \u54728 \u50948 , \u44592 \u51116 \u45572 \u46973 , \u50724 \u44592 \u44032  \u51080 \u45716  \u44221 \u50864 \u8232 2.\'a0\u49345 \u54665 \u50948 (\u51116 \u54032 \u47588 )\u47484  \u47785 \u51201 \u51004 \u47196  \u44396 \u47588 \u54616 \u45716  \u44144 \u47000 \u51060 \u44144 \u45208 , \u44144 \u47000  \u51221 \u54889 \u49345  \u49345 \u54665 \u50948 (\u51116 \u54032 \u47588 )\u47484  \u47785 \u51201 \u51004 \u47196  \u54620  \u44396 \u47588 \u51064  \u44221 \u50864 \u8232 3.\'a0\u44592 \u53440  \u44396 \u47588 \u49888 \u52397 \u51012  \u49849 \u45209 \u54616 \u45716  \u44163 \u51060  \u44288 \u47532 \u51201 , \u44592 \u49696 \u51201 \u51004 \u47196  \u54788 \u51200 \u55176  \u51648 \u51109 \u51060  \u51080 \u45716  \u44221 \u50864 \u8232 \u9313 \'a0\u44396 \u47588 \u44228 \u50557 \u51008  \u54924 \u49324 \u51032  \u49849 \u45209 \u51060  \u48376  \u50557 \u44288  \u51228 11\u51312  \u51228 1\u54637 \u51032  \u49688 \u49888 \u54869 \u51064  \u53685 \u51648 \u54805 \u53468 \u47196  \u51060 \u50857 \u51088 \u50640 \u44172  \u46020 \u45804 \u54620  \u49884 \u51216 \u50640  \u49457 \u47549 \u54633 \u45768 \u45796 .\u8232 \u9314 \'a0\u54924 \u49324 \u51032  \u49849 \u45209 \u51032  \u51032 \u49324 \u54364 \u49884 \u50640 \u45716  \u51060 \u50857 \u51088 \u51032  \u44396 \u47588 \u49888 \u52397 \u50640  \u45824 \u54620  \u54869 \u51064  \u48143  \u54032 \u47588 \u44032 \u45733  \u50668 \u48512 , \u44396 \u47588 \u49888 \u52397 \u51032  \u51221 \u51221  \u52712 \u49548  \u46321 \u50640  \u44288 \u54620  \u51221 \u48372  \u46321 \u51012  \u54252 \u54632 \u54616 \u50668 \u50556  \u54633 \u45768 \u45796 . \
